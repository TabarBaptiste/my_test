{% extends 'base.html.twig' %}

{% block title %}{{ produit.title }}{% endblock %}
{% block body %}
<div class="container">
    <h1>{{ produit.title }}</h1>
    <div class="card">
        <div class="card-body">
            <p class="card-text">{{ produit.description }}</p>
            <p class="card-text">{{ produit.dateCreated|date('d/m/Y') }}</p>
        </div>
    </div>
</div>
{% endblock %}